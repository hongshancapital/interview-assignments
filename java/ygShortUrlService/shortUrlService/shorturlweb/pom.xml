<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 https://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>
    <parent>
        <groupId>com.hongshang</groupId>
        <artifactId>shortUrlService</artifactId>
        <version>0.0.1-SNAPSHOT</version>
    </parent>
    <groupId>com.hongshang</groupId>
    <artifactId>shorturlweb</artifactId>
    <version>0.0.1-SNAPSHOT</version>
    <name>shorturlweb</name>
    <description>ShortUrlServer project</description>
    <properties>
        <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
        <project.reporting.outputEncoding>UTF-8</project.reporting.outputEncoding>
        <java.version>1.8</java.version>
    </properties>
    <dependencies>

        <dependency>
            <groupId>org.springframework.boot</groupId>
            <artifactId>spring-boot-starter-web</artifactId>
        </dependency>

        <dependency>
            <groupId>org.springframework.boot</groupId>
            <artifactId>spring-boot-starter-test</artifactId>
            <scope>test</scope>
        </dependency>

        <!-- 添加 shorturlimpl 的依赖 -->
        <dependency>
            <groupId>com.hongshang</groupId>
            <artifactId>shorturlimpl</artifactId>
            <version>0.0.1-SNAPSHOT</version>
            <scope>compile</scope>
        </dependency>

        <!-- 添加 shorturlinterface 的依赖 -->
        <dependency>
            <groupId>com.hongshang</groupId>
            <artifactId>shorturlinterface</artifactId>
            <version>0.0.1-SNAPSHOT</version>
        </dependency>

        <!-- 添加 common 的依赖 -->
        <dependency>
            <groupId>com.hongshang</groupId>
            <artifactId>common</artifactId>
            <version>0.0.1-SNAPSHOT</version>
        </dependency>

    </dependencies>

    <build>
        <plugins>
            <plugin>
                <groupId>org.springframework.boot</groupId>
                <artifactId>spring-boot-maven-plugin</artifactId>
            </plugin>
        </plugins>
    </build>

</project>
