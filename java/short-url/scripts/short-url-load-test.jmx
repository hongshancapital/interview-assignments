            <stringProp name="cacheKey">true</stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="script">import org.apache.commons.io.FileUtils

def resultCount = vars.get(&quot;id_matchNr&quot;)
log.warn &apos;Output to jmeter console&apos; +  resultCount

// Create file Object
f = new File(&quot;./ids.txt&quot;)
content = &quot;&quot;;
FileUtils.writeStringToFile(f, content, false)

for (int i=1; i&lt;=resultCount.toInteger(); i++) {
	id = vars.get(&quot;id_&quot;+i)
	content += id + System.getProperty(&quot;line.separator&quot;);
}

FileUtils.writeStringToFile(f, content, true)
</stringProp>
            <stringProp name="scriptLanguage">groovy</stringProp>
          </JSR223PostProcessor>
