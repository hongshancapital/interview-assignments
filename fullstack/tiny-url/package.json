{
  "name": "url_shorterner",
  "version": "1.0.0",
  "description": "Url Shorterner(TypeScript)",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "tsnd": "tsnd --debug --clear --rs src/debug.ts",
    "concurrently": "concurrently \"npx tsc --watch\" \"nodemon -q dist/index.js\"",
    "dev:test": "NODE_ENV=test nodemon",
    "dev": "nodemon",
    "prestart": "npm run build",
    "start": "node dist/index.js",
    "build": "cross-env NODE_ENV=production tsc",
    "lint": "eslint src --fix --ext .ts,.tsx",
    "test": "cross-env NODE_ENV=test jest  --detectOpenHandles --runInBand",
    "test:watchall": "cross-env NODE_ENV=test jest  --watchAll --detectOpenHandles --runInBand",
    "cov": "cross-env NODE_ENV=test jest --coverage --detectOpenHandles"
  },
  "author": "king(wechat:deepdev)",
  "license": "ISC",
  "start": "node dist/index.js",
  "dev": "nodemon dist/index.js",
  "build": "tsc -p . && cp .env dist/.env",
  "dependencies": {
    "base62": "^2.0.1",
    "body-parser": "^1.19.2",
    "chalk": "^4.1.2",
    "config": "^3.3.7",
    "console-stamp": "^3.0.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "helmet": "^5.0.2",
    "ioredis": "^5.0.4",
    "js-yaml": "^4.1.0",
    "mongoose": "^6.2.11",
    "mysql2": "^2.3.3",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.18.0",
    "sequelize-typescript": "^2.1.3",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/config": "^0.0.41",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^17.0.23",
    "@types/sequelize": "^4.28.11",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "concurrently": "^7.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.13.0",
