{
  "name": "short-url",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "rimraf bin && tsc",
    "dev": "ts-node src/start.ts",
    "dev:watch": "nodemon --watch 'src/**/*.ts' --ignore 'src/**/*.spec.ts' --exec ts-node src/start.ts",
    "dev:debug": "node --require ts-node/register --inspect src/start.ts",
    "lint": "eslint . --fix",
    "prestart": "npm run build",
    "start": "node bin/start.js",
    "docker:up": "docker-compose -p test -f ./test/docker-compose.yml --env-file ./test/.env.test up -d",
    "docker:down": "docker-compose -p test -f ./test/docker-compose.yml --env-file ./test/.env.test down",
    "test": "npm run test:all",
    "test:ci": "CI=true npm run test:all",
    "test:all": "ENV_FILE=./test/.env.test jest -i --coverage",
    "test:int": "ENV_FILE=./test/.env.test jest -i --testRegex src/__test__/*.spec.ts",
    "test:unit": "SKIP_SETUP_DOCKER_COMPOSE=true jest -i --testPathIgnorePatterns src/__test__",
    "prisma": "prisma generate",
    "prisma:migrate:test": "dotenv -e ./test/.env.test prisma migrate deploy"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^4.8.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "express-winston": "^4.2.0",
    "hashids": "^2.2.10",
    "helmet": "^6.0.1",
    "rimraf": "^3.0.2",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.15",
    "@types/express-serve-static-core": "^4.17.32",
    "@types/jest": "^29.2.5",
    "@types/node": "^18.11.18",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "axios": "^1.2.2",
    "docker-compose": "^0.23.17",
    "dotenv-cli": "^6.0.0",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "is-ci": "^3.0.1",
    "is-reachable": "^5.2.1",
    "jest": "^29.3.1",
    "jest-mock-extended": "^3.0.1",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.1",
    "prisma": "^4.8.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.9.4",
    "zod": "^3.20.2"
  }
}
