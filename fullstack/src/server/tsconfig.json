{
    "compilerOptions": {
        "module": "NodeNext",
        "target": "ESNext",
        "sourceMap": true,
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "types": [
            "node",
            "express"
        ],
        "moduleResolution": "NodeNext",
        "strict": true,
        "noImplicitAny": true,
        "strictNullChecks": true,
        "strictBindCallApply": true,
        "strictFunctionTypes": true,
        "strictPropertyInitialization": true,
    },
    "exclude": [
        "../../node_modules"
    ]
}
