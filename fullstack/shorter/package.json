{
  "name": "shorter",
  "version": "1.0.0",
  "description": "a service for transform long url to it's shorter alias",
  "main": "./dist/index.js",
  "types": "./index.ts",
    "scripts": {
        "build": "npx tsc",
        "start": "node dist/index.js",
        "dev": "concurrently \"npx tsc --watch\" \"nodemon -q dist/index.js\""
    },
  "keywords": [
    "shorter"
  ],
  "author": "Feng Li",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.15",
    "@types/node": "^18.11.18",
    "concurrently": "^7.6.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "nodemon": "^2.0.20",
    "typescript": "^4.9.4"
  }
}
