{
  "name": "fullstack-assignment",
  "version": "1.0.2",
  "scripts": {
    "start": "npm install && npm run build && npm run serve",
    "serve": "node build/server.js",
    "build": "npm run clean && npm run build-ts",
    "clean": "rimraf ./build",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "eslint": "eslint . --ext .ts",
    "upgrade": "npm update --save-dev && npm update --save",
    "test": "npm install && jest --forceExit --detectOpenHandles --coverage --verbose"
  },
  "dependencies": {
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "delay": "^5.0.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "express": "^4.18.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.4",
    "@types/node": "^14.14.6",
    "@types/supertest": "^2.0.12",
    "ts-jest": "^26.4.3",
    "jest": "^26.6.1",
    "supertest": "^6.0.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.5"
  }
}
