generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model User {
  id    Int     @id @default(autoincrement())
  email String  
  name  String?
  links Links[]
}

model Links {
  id        Int      @id @default(autoincrement())
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  shortLink String  @unique
  longLink String   
  user    User?    @relation(fields: [userId], references: [id])
  userId  Int?
}