{
  "name": "shortdomain",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "nodemon",
    "build": "rimraf ./dist && tsc",
    "test:unit": "NODE_ENV=dev mocha --nolazy -r ts-node/register test/unit/**/*.test.ts",
    "test:integration": "NODE_ENV=dev mocha --nolazy --timeout 7000 -r ts-node/register test/integration/**/*.test.ts",
    "test": "npm run test:unit && npm run test:integration",
    "cover": "nyc npm run test"
  },
  "dependencies": {
    "crc-32": "^1.2.2",
    "express": "~4.16.1",
    "ioredis": "^5.3.0",
    "knex": "^2.4.2",
    "mysql": "^2.18.1"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@types/express": "^4.17.17",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.13.0",
    "@types/sinon": "^10.0.13",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "eslint": "^8.33.0",
    "eslint-config-standard-with-typescript": "^34.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-promise": "^6.1.1",
    "mocha": "^10.2.0",
    "nodemon": "^2.0.20",
    "nyc": "^15.1.0",
    "rimraf": "^4.1.2",
    "sinon": "^15.0.1",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  }
}
