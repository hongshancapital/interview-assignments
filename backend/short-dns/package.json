    "build-ts": "tsc -p tsconfig.json",
    "start": "./node_modules/nodemon/bin/nodemon.js",
    "clean": "rm -rf .nyc_output report dist",
    "lint": "eslint -c .eslintrc.prod.json -f compact --cache --ext .ts,.js src",
    "lint-fix": "eslint -c .eslintrc.prod.json -f compact --cache --ext .ts,.js src --fix",
    "test": "NODE_ENV=test nyc mocha \"src/test/*.unit.test.ts\" --parallel",
    "check": "npm run check-ts && npm run lint && npm run test",
    "check-ts": "tsc -p tsconfig.json --noEmit"
  },
  "keywords": [
    "shortDNS"
  ],
  "author": "Wang Jian",
